<div id="container" {{bind-attr class="showingResults"}}>
  <div id="inner-container">

  <div id="players">
    <div id="timelineMixImageCell">
        {{partial 'icons/logo'}}
    </div>

    <div id="timeline" {{bind-attr class="showingResults:disabled:enabled"}}>
      <!-- 2010s -->
      <div class="decade current-decade">
        {{view 'year' prelink="20" link="15" year="2015" }}
        {{view 'year' link="14" year="2014" }}
        {{view 'year' link="13" year="2013" }}
        {{view 'year' link="12" year="2012" }}
        {{view 'year' link="11" year="2011" }}
        {{view 'year' prelink="20" link="10" year="2010" }}
      </div>

      <!-- 00s -->
      <div class="decade">
        {{view 'year' prelink="20" link="09" year="2009" }}
        {{view 'year' link="08" year="08" }}
        {{view 'year' link="07" year="07" }}
        {{view 'year' link="06" year="06" }}
        {{view 'year' link="05" year="05" }}
        {{view 'year' link="04" year="04" }}
        {{view 'year' link="03" year="03" }}
        {{view 'year' link="02" year="02" }}
        {{view 'year' link="01" year="01" }}
        {{view 'year' prelink="19" link="00" year="2000" }}
      </div>

      <!-- 90s -->
      <div class="decade">
        {{view 'year' prelink="19" link="99" year="1999" }}
        {{view 'year' link="98" year="98" }}
        {{view 'year' link="97" year="97" }}
        {{view 'year' link="96" year="96" }}
        {{view 'year' link="95" year="95" }}
        {{view 'year' link="94" year="94" }}
        {{view 'year' link="93" year="93" }}
        {{view 'year' link="92" year="92" }}
        {{view 'year' link="91" year="91" }}
        {{view 'year' prelink="19" link="90" year="1990" }}
      </div>

      <!-- earlier/other -->
      <div class="decade">
        {{view 'year' prelink="19" link="89" year="1989" }}
        {{view 'year' prelink="19" link="88" year="1988" }}
      </div>
    </div>
  </div>

  
    {{#if showingResults}}
    {{ view 'year-select'  }}
    {{/if}}

  </div>
</div>

<div id="broadcasts-by-month" {{bind-attr class=":sidepanel showingResults:enabled:disabled"}}>

  {{#if hasHighlights}}
    <p {{action 'viewFeatured'}}>
      View {{#if getHighlights }}all{{else}}highlights{{/if}}
    </p>
  {{/if}}


  {{#each group in groupedContent}}
    <div class="month">
    <h2>{{group.group.key}}</h2>
    {{#each cont in group.content}}
      {{render "sidepanel-mix" cont}}
    {{/each}}
    </div>
  {{/each}}

 
</div>