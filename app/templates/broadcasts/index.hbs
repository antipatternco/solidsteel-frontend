      <p>{{#link-to "broadcasts" (query-params year="1992") }}1992{{/link-to}}</p>
    </div>


  </div>
</div>

<div id="broadcasts-by-month" {{bind-attr class=":sidepanel showingResults:enabled:disabled"}}>
  <h2>{{year}}</h2>
  {{#each group in groupedContent}}
    <div class="month">
    <h3>{{group.group.key}}</h3>
    {{#each cont in group.content}}
      
      {{render "SidepanelMix" cont}}


    {{/each}}
    <br /><br />
    </div>
  {{/each}}
</div>